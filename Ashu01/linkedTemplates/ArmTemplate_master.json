{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "Ashu01"
		},
		"Airline_linked_service_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'Airline_linked_service'"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"AzureDataLakeStorage2Taxi_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage2Taxi'"
		},
		"covid19_01_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'covid19_01'"
		},
		"covid19tg_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'covid19tg'"
		},
		"AIRLINE_RESTAPI_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://airlabs.co/api/v9/flights"
		},
		"Airline_linked_service_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://airportdatalake01.dfs.core.windows.net/"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://covid01.dfs.core.windows.net/"
		},
		"AzureDataLakeStorage2Taxi_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://taxistores.dfs.core.windows.net/"
		},
		"HttpServer_ucdc_covid19_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().sourceBaseURL}"
		},
		"Taxi_Ansh_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://d37ci6vzurychx.cloudfront.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "Ashu01_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Airline_linked_service_accountKey": {
						"value": "[parameters('Airline_linked_service_accountKey')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"AzureDataLakeStorage2Taxi_accountKey": {
						"value": "[parameters('AzureDataLakeStorage2Taxi_accountKey')]"
					},
					"covid19_01_connectionString": {
						"value": "[parameters('covid19_01_connectionString')]"
					},
					"covid19tg_connectionString": {
						"value": "[parameters('covid19tg_connectionString')]"
					},
					"AIRLINE_RESTAPI_properties_typeProperties_url": {
						"value": "[parameters('AIRLINE_RESTAPI_properties_typeProperties_url')]"
					},
					"Airline_linked_service_properties_typeProperties_url": {
						"value": "[parameters('Airline_linked_service_properties_typeProperties_url')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"AzureDataLakeStorage2Taxi_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage2Taxi_properties_typeProperties_url')]"
					},
					"HttpServer_ucdc_covid19_properties_typeProperties_url": {
						"value": "[parameters('HttpServer_ucdc_covid19_properties_typeProperties_url')]"
					},
					"Taxi_Ansh_properties_typeProperties_url": {
						"value": "[parameters('Taxi_Ansh_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "Ashu01_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'Ashu01_ArmTemplate_0')]"
			]
		}
	]
}